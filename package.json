{
  "name": "bbh-chatbox-hybrid-chat",
  "version": "1.0.42",
  "private": true,
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "dev": "vite --mode dev",
    "staging": "vite --mode staging",
    "prod": "vite --mode prod",
    "build-dev": "run-p type-check build-only-dev",
    "build-staging": "run-p type-check build-only-staging",
    "build-prod": "run-p type-check build-only-prod",
    "build": "run-p type-check build-only-prod",
    "build-retion": "VITE_BASE_URL=/chat run-s build zip",
    "build-only-dev": "vite build --mode dev",
    "build-only-staging": "vite build --mode staging",
    "build-only-prod": "vite build --mode prod",
    "type-check": "vue-tsc --noEmit",
    "deploy": "./deploy.sh",
    "zip": "zip -r dist.zip dist"
    
  },
  "dependencies": {
    "@chenfengyuan/vue-qrcode": "^2.0.0",
    "async": "^3.2.4",
    "autoprefixer": "^10.4.13",
    "date-fns": "2.30.0",
    "lodash": "^4.17.21",
    "pinia": "^2.0.28",
    "postcss": "^8.4.19",
    "query-string": "^8.1.0",
    "sass": "^1.56.2",
    "sweetalert2": "^11.7.12",
    "tailwindcss": "^3.2.4",
    "vue": "^3.4.27",
    "vue-cleave-component": "^3.0.2",
    "vue-datepicker-next": "^1.0.2",
    "vue-i18n": "9.13.1",
    "vue-router": "4.3.3",
    "vue-virtual-scroller": "2.0.0-beta.8",
    "vue3-virtual-scroll-list": "^0.2.1",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@types/async": "^3.2.16",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.16.18",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/tsconfig": "^0.1.3",
    "npm-run-all": "^4.1.5",
    "typescript": "~4.7.4",
    "vite": "^4.0.0",
    "vue-tsc": "^1.0.12"
  }
}
